{
  "priority": 1,
  "request": {
    "method": "POST",
    "urlPattern": "/rest/api/3/issue/[^/]+/attachments",
    "headers": {
      "Content-Type": {
        "matches": "multipart/form-data.*"
      },
      "Authorization": {
        "matches": "Basic .*"
      },
      "X-Atlassian-Token": {
        "equalTo": "no-check"
      }
    },
    "multipartPatterns": [
      {
        "name": "file",
        "equalTo": "TicketBookingApi_Error.png"
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "JiraAddAttachmentResponseBody_200.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
